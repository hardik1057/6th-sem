   "source": [
    "#solve a system of linear equation using numpy functions \n",
    "import numpy as np\n",
    "A = np.array([[4, 3], [-5, 9]]) #Coeff\n",
    "B = np.array([20, 26]) #Constant\n",
    "X = np.linalg.solve(A, B)\n",
    "x, y = X\n",
    "print(f\"Solution: x = {x}, y = {y}\")\n"
   ]
  }
